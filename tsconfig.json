{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es6",
      "dom"
    ],
    "outDir": "./lib",
    "baseUrl": "./src",
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "removeComments": true, // 是否移除注释
    "esModuleInterop": true
  },
  "exclude": ["node_modules"]
}